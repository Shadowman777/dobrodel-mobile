  "scripts": {
    "android": "ENVFILE=.env.production react-native run-android",
    "android:dev": "ENVFILE=.env.development react-native run-android",
    "ios": "ENVFILE=.env.production react-native run-ios --simulator='iPhone 7'",
    "ios:dev": "ENVFILE=.env.development react-native run-ios --simulator='iPhone 7'",
    "ios:device": "ENVFILE=.env.production react-native run-ios --device",
    "ios:dev:device": "ENVFILE=.env.development react-native run-ios --device",
    "pod": "cd ios && pod install && cd ..",
    "build": "cd android && ENVFILE=.env.production ./gradlew assembleRelease && cd ..",
    "build:dev": "cd android && ENVFILE=.env.development ./gradlew assembleRelease && cd ..",
    "build:debug": "cd android && ENVFILE=.env.production ./gradlew assembleDebug && cd ..",
    "bundle": "cd android && ENVFILE=.env.production ./gradlew bundleRelease && cd ..",
    "start": "react-native start",
    "clean": "watchman watch-del-all && rm -rf node_modules && rm -rf yarn.lock && rm -rf ios/Pods && yarn install && yarn pod && watchman watch-project .",
    "test": "jest",
    "wrapper": "cd android && gradle wrapper && cd ..",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
